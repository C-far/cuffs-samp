/*
FAKE NATIVE FUNCTIONS

native SetPlayerCuffed(playerid, bool:cuffed = true);
*/


stock bool:IsPlayerCuffed(playerid)
{
	// Check if the pVar 'CUFF_object_index' is NOT 0.
	return (GetPVarType(playerid, "CUFF_object_index") != 0);
}

// SetPlayerCuffed(playerid, bool:cuffed = true)
// If cuffed is 'true', cuffs are attached. If they are already attached 0 is returned. If they are successfully attached, 1 is returned.
// If cuffed is 'false' cuffs are removed. If the player isn't cuffed, 0 is returned. If they are successfully removed, 1 is returned.
// If the playerid passed to SetPlayerCuffed is not connected, -2 is returned.
// If there is no slot available for the cuffs object, -1 will be returned.

stock SetPlayerCuffed(playerid, bool:cuffed = true)
{
	if(!IsPlayerConnected(playerid) || playerid == INVALID_PLAYER_ID) return INVALID_PLAYER_ID; // Just in case...

	if(cuffed == IsPlayerCuffed(playerid)) return 0; // Trying to cuff them when they're already cuffed (or uncuffing when not cuffed)
	
	if(cuffed) // Cuff them, if they aren't already
	{
		for(new i=MAX_PLAYER_ATTACHED_OBJECTS-1; i!=0; i--)
		{
			if(!IsPlayerAttachedObjectSlotUsed(playerid, i)) // Free slot
			{
				new skin = GetPlayerSkin(playerid);
				SetPVarInt(playerid, "CUFF_object_index", i);
				
				SetPlayerSpecialAction(playerid, SPECIAL_ACTION_CUFFED);

				SetPlayerAttachedObject(playerid, i, 19418, 6, CUFF_CuffObjectOffsets[skin][0], CUFF_CuffObjectOffsets[skin][1], CUFF_CuffObjectOffsets[skin][2], CUFF_CuffObjectOffsets[skin][3], CUFF_CuffObjectOffsets[skin][4], CUFF_CuffObjectOffsets[skin][5], CUFF_CuffObjectOffsets[skin][6], CUFF_CuffObjectOffsets[skin][7], CUFF_CuffObjectOffsets[skin][8]);
				return 1;
			}
		}
		return -1; // No slot available
	}
	else // Un-cuff them
	{
		RemovePlayerAttachedObject(playerid, GetPVarInt(playerid, "CUFF_object_index"));
		DeletePVar(playerid, "CUFF_object_index");
		SetPlayerSpecialAction(playerid, 0);
		return 1; // Successfully attached
	}
}

stock CUFF_RemovePlayerAttachedObject(playerid, index)
{
	// Don't allow external scripts to remove the cuffs attached object (use SetPlayerCuffed!)
	if(IsPlayerCuffed(playerid) && GetPVarInt(playerid, "CUFF_object_index") == index) return 0;

	return RemovePlayerAttachedObject(playerid, index);
}
#if defined _ALS_RemovePlayerAttachedObject
	#undef RemovePlayerAttachedObject
#else
	#define _ALS_RemovePlayerAttachedObject
#endif
#define RemovePlayerAttachedObject CUFF_RemovePlayerAttachedObject

// Hook SetPlayerSkin so the cuffs can be reset on skin change
stock CUFF_SetPlayerSkin(playerid, skin)
{
	if(IsPlayerCuffed(playerid))
	{
		new CUFF_id = GetPVarInt(playerid, "CUFF_object_index");
		SetPlayerAttachedObject(playerid, CUFF_id, 19418, 6, CUFF_CuffObjectOffsets[skin][0], CUFF_CuffObjectOffsets[skin][1], CUFF_CuffObjectOffsets[skin][2], CUFF_CuffObjectOffsets[skin][3], CUFF_CuffObjectOffsets[skin][4], CUFF_CuffObjectOffsets[skin][5], CUFF_CuffObjectOffsets[skin][6], CUFF_CuffObjectOffsets[skin][7], CUFF_CuffObjectOffsets[skin][8]);
	}
	return SetPlayerSkin(playerid, skin);
}
#if defined _ALS_SetPlayerSkin
	#undef SetPlayerSkin
#else
	#define _ALS_SetPlayerSkin
#endif
#define SetPlayerSkin CUFF_SetPlayerSkin

